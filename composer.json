{
    "name": "dillingham/stubs",
    "description": "Create files, folders & content with variables",
    "keywords": [
        "php",
        "stub"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "symfony/console": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Stub\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Stub\\Providers\\LaravelProvider"
            ]
        }
    },
    "bin": [
        "stub"
    ]
}